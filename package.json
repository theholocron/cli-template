{
  "name": "@theholocron/cli-template",
  "description": "<description>",
  "homepage": "https://github.com/theholocron/cli-template#readme",
  "author": "Newton Koumantzelis",
  "main": "src/index.ts",
  "module": "dist/cli-template.es.js",
