    "@theholocron/storybook-config": "^3.4.4",
    "@theholocron/stylelint-config": "^3.4.4",
    "@theholocron/tsconfig": "^3.4.4",
    "@tsconfig/node-lts": "^20.1.3",
    "alexjs": "^1.0.0",
    "vite": "^5.4.9"
  }
