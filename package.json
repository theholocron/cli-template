  "wiki": "https://github.com/theholocron/cli-template/wiki",
  "license": "GPL-3.0",
  "packageManager": "npm@9.5.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "marked-man README.md > man/cli-template.1",
